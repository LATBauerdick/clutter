{
  "name": "clutter-frontend",
  "description": "The frontend for the clutter app",
  "author": "LATBauerdick",
  "license": "MIT",
  "scripts": {
    "postinstall": "spago install",
    "clean": "rm -rf node_modules output .spago dist/*.js *.lock .cache",
    "build": "spago build",
    "test": "spago test",
    "serve": "spago build && esbuild --bundle src/index.js --outfile=dist/index.js --servedir=dist",
    "bundle": "spago build --output output-es && purs-backend-es bundle-app --main Main --minify --no-build --to dist/index.js"
  },
  "dependencies": {
    "benchmark": "^2.1.4",
    "parcel": "2.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "esbuild": "0.23.1",
    "parcel": "2.8.0",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "url": "^0.11.0",
    "xhr2": "^0.2.1"
  }
}
